  "devDependencies": {
    "deep-freeze-strict": "^1.1.1",
    "expect": "^1.20.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "redux-mock-store": "^1.2.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "copyfiles": "^1.0.0",
    "firebase": "^3.6.4",
    "foundation-sites": "^6.2.4",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "node-sass": "^4.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-scripts": "0.8.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "uuid": "^3.0.1"
  },
  "scripts": {
    "_to_slow_prestart": "npm unlink react-router&&npm run router_clone&&npm run router_build",
    "router_clone": "git clone https://github.com/ReactTraining/react-router.git node_modules/react-router",
    "router_build": "cd node_modules/react-router&&git checkout -b v4 553b56a&&npm install --dev&&npm run build-lib",
    "react-router_hack": "rimraf node_modules/react-router&&copyfiles react-router/* node_modules&&cd node_modules/react-router&&npm install --only=prod",
    "heroku-postbuild": "npm run react-router_hack",
    "start": "npm run scss&&react-scripts build&&node server.js",
    "start_dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test_karma": "karma start",
    "eject": "react-scripts eject",
    "scss": "node-sass --include-path node_modules/foundation-sites/scss --recursive src/styles -o src",
    "scssw": "node-sass -w --include-path node_modules/foundation-sites/scss --recursive src/styles -o src"
  }
}
